{
    "name": "aidantwoods/preparedhtml",
    "description": "HTML with context boundaries",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aidan Woods",
            "email": "aidantwoods@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aidantwoods\\PreparedHTML\\": [
                "src/"
            ]
        }
    },
    "require": {
        "php": "^7.0"
    },
    "suggest": {
        "aidantwoods/secureheaders": "To enable browser security features."
    },
    "autoload-dev": {
        "psr-4": {
            "Aidantwoods\\PreparedHTML\\Tests\\": [
                "tests/"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
